// Test secrets precedence (.env and rq__ env vars override environment variables)

env local {
    secret_value: "from_env_block";
    os_token: "from_env_block";
}

// File-level fallback (lowest)
let secret_value = "from_file";
let os_token = "from_file";

rq secrets_demo("http://localhost:8080/api/secret-test", [
    "X-Secret": "{{secret_value}}",
    "X-Os-Token": "{{os_token}}"
]);