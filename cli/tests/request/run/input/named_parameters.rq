// Test file demonstrating named parameter syntax
rq named_params_request(
    url: "http://localhost:8080/get", 
    headers: [
        "Accept": "application/json",
        "X-Test": "named-params"
    ],
    body: ${"test": "named_parameters"}
);

rq named_partial(url: "http://localhost:8080/simple");

rq mixed_params("http://localhost:8080/mixed", headers: [
    "X-Mixed": "positional-url-named-headers"
]);

ep named_endpoint(
    url: "http://localhost:8080/api",
    headers: [
        "Authorization": "Bearer token123",
        "Content-Type": "application/json"
    ]
) {
    let endpoint_var = "from_endpoint";
    
    rq child_request("/users");
    rq child_named(url: "/posts", headers: ["X-Child": "overrides"]);
}