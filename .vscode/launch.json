{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Run RQ Extension",
			"type": "extensionHost",
			"request": "launch",
			"args": [
				"--extensionDevelopmentPath=${workspaceFolder}/vscode-extension",
				"${workspaceFolder}"
			],
			"outFiles": ["${workspaceFolder}/vscode-extension/out/**/*.js"],
			"preLaunchTask": "Extension Watch"
		}
	]
}
