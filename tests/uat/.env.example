BASE_URL=http://localhost:8080

# Bearer token used by bearer_env (auth_name = bearer_auth)
API_TOKEN=some-token

# OAuth2 Authorization Code / Implicit flows (used by oauth_ac_*_env and oauth_implicit_default_env)
OAUTH_CLIENT_ID=replace-me
OAUTH_CLIENT_SECRET=replace-me
OAUTH_AUTHORIZATION_URL=http://localhost:9090/realms/rq-uat/protocol/openid-connect/auth
OAUTH_TOKEN_URL=http://localhost:9090/realms/rq-uat/protocol/openid-connect/token
OAUTH_SCOPE=read:all

# OAuth2 Client Credentials with certificate (used by oauth_cc_cert_env)
OAUTH_CC_CERT_CLIENT_ID=rq-uat-cc-cert
OAUTH_CC_CERT_PASSWORD=rq-uat-p12
